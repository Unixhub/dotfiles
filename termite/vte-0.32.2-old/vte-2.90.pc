prefix=/opt/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

defaultemulation=@VTE_DEFAULT_EMULATION@

Name: vte
Description: Vte terminal widget.
Version: 0.32.2
Requires: glib-2.0 >= 2.31.13 gobject-2.0 pango >= 1.22.0 gtk+-3.0 >= 3.1.9 gobject-2.0 gio-2.0 gio-unix-2.0  x11 cairo-xlib
Libs: -L${libdir} -lvte2_90
Cflags: -I${includedir}/vte-2.90 -DVTE_SEAL_ENABLE
